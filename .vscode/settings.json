{
  "[css]": {
    "editor.defaultFormatter": "biomejs.biome",
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[json5]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "biome.enabled": true,
  "cSpell.words": [
    "cfonts",
    "committerdate",
    "cristal",
    "deepseek",
    "inkjs",
    "rolldown",
    "THUDM",
    "tinyexec",
    "tsdown",
    "unconfig",
    "worktree",
    "worktrees"
  ],
  "css.customData": [".vscode/tailwind.json"],
  "css.format.enable": false,
  "editor.codeActionsOnSave": {
    "quickfix.biome.correctness.noUnusedImports": "explicit",
    "quickfix.biome.correctness.noUnusedVariables": "explicit",
    "source.action.useSortedAttributes.biome": "explicit",
    "source.action.useSortedKeys.biome": "explicit",
    "source.action.useSortedProperties.biome": "explicit",
    "source.fixAll.biome": "explicit",
    "source.fixAll.eslint": "never",
    "source.fixAll.ts": "never",
    "source.organizeImports": "never",
    "source.organizeImports.biome": "explicit",
    "source.removeUnused.ts": "never",
    "source.removeUnusedImports": "never",
    "source.removeUnusedVariables": "never",
    "source.addMissingImports.ts": "never",
    "source.addMissingTypeImports.ts": "never",
    "source.sortImports": "never",
    "source.biome": "explicit",
    "source.organizeLinkDefinitions": "never"
  },
  "editor.defaultFormatter": "biomejs.biome",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 3000,
  "files.autoSaveWhenNoErrors": true
}
