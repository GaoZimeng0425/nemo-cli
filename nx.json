{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "build-dts",
          "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "check": {
      "dependsOn": [
        "^check"
      ]
    },
    "build-dts": {
      "dependsOn": [
        "^build-dts"
      ]
    }
  }
}
