{
  "name": "@nemo-cli/ai",
  "version": "0.0.1",
  "description": "AI CLI",
  "author": "gaozimeng <gaozimeng0425@gmail.com>",
  "homepage": "",
  "license": "ISC",
  "type": "module",
  "bin": {
    "na": "./bin/index.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev:slack": "tsx src/services/slack/bot.ts",
    "dev": "rolldown --watch -c ./rolldown.config.ts",
    "build": "rolldown -c ./rolldown.config.ts",
    "build-dts": "rolldown -c ./rolldown.config.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check": "tsc --incremental --noEmit",
    "prepublish": "npm run build",
    "prepack": "rolldown"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "^1.0.20",
    "@ai-sdk/google": "^2.0.17",
    "@ai-sdk/openai": "^2.0.42",
    "@nemo-cli/git": "workspace:*",
    "@nemo-cli/mail": "workspace:*",
    "@nemo-cli/shared": "workspace:*",
    "@nemo-cli/ui": "workspace:*",
    "ai": "^5.0.59",
    "confluence.js": "^2.1.0",
    "fastmcp": "^3.18.0"
  }
}
