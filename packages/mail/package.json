{
  "name": "@nemo-cli/mail",
  "version": "0.0.1",
  "description": "Mail CLI",
  "author": "gaozimeng <gaozimeng0425@gmail.com>",
  "homepage": "",
  "license": "ISC",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev:email": "email dev",
    "dev": "rolldown --watch -c ./rolldown.config.ts",
    "build": "rolldown -c ./rolldown.config.ts",
    "build-dts": "rolldown -c ./rolldown.config.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check": "tsc --incremental --noEmit",
    "prepublish": "npm run build",
    "prepack": "rolldown"
  },
  "dependencies": {
    "@nemo-cli/shared": "workspace:*",
    "@nemo-cli/ui": "workspace:*",
    "@react-email/components": "^0.5.5",
    "nodemailer": "^7.0.6",
    "react": "catalog:"
  },
  "devDependencies": {
    "@react-email/preview-server": "4.2.12",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "catalog:",
    "react-email": "^4.2.12"
  }
}
